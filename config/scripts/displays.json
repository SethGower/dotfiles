{
    "configurations": {
        "laptop_docked": {
            "outputs": ["DP-3", "DP-5", "eDP-1-1"],
            "command": "xrandr --output DP-3 --auto  --output DP-5 --auto --right-of DP-3 --primary  --output eDP-1-1 --off; feh --bg-fill ~/.wallpapers/bully_hill.jpg"
        },
        "laptop_undocked": {
            "outputs": ["eDP1"],
            "transition_output": "eDP1",
            "command": "xrandr --output eDP1 --primary --auto; feh --bg-fill ~/.wallpapers/bully_hill.jpg"
        },
        "laptop_undocked1": {
            "outputs": ["eDP-1"],
            "transition_output": "eDP-1",
            "command": "xrandr --output eDP-1 --primary --auto; feh --bg-fill ~/.wallpapers/bully_hill.jpg"
        }
    }
}
